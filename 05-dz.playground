//Домашнее задание по теме условные операторы
let count : UInt=14
let apples:String
if count%10==1 {
    if count%100==11 {
        apples="яблок"
    } else {
        apples="яблоко"
    }
    
} else if count%10>1 && count%10<5 {
    if count%100>=11 && count%100<=14 {
        apples="яблок"
    } else {
        apples="яблока"
    }
} else {
    apples="яблок"
}

let result = "В корзине \(count) \(apples)"
print(result)
